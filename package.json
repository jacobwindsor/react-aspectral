{
  "name": "react-aspectral",
  "version": "1.0.0",
  "description": "Automatically resize React elements to a given aspect ratio",
  "repository": "https://github.com/jacobwindsor/react-aspectral",
  "main": "lib/index.js",
  "author": "Jacob Windsor",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react": "^15.6.1",
    "react-resize-detector": "^0.5.0"
  },
  "babel": {
    "plugins": [
      "transform-class-properties"
    ],
    "presets": [
      "env",
      "react"
    ]
  },
  "scripts": {
    "build": "babel index.js -d lib",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0"
  }
}
